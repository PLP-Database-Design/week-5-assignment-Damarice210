<table>
   <thead>
       <tr>
           <th>patient_id</th>
           <th>first_name</th>
           <th>date_of_birth</th>
           <th>last_name</th>
       </tr>
   </thead>
   <tbody>
       <% results.forEach(row => { %>
           <tr>
               <td><%= row.patient_id %></td>
               <td><%= row.first_name %></td>
               <td><%= row.date_of_birth %></td>
               <td><%= row.last_name %></td>
           </tr>
       <% }); %>
   </tbody>
</table>

